#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = Exemplo5.exe Project1.exe Exemplo5.exe
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

Exemplo5.exe: Exemplo5.dpr
  $(DCC)

Project1.exe: ..\Exemplo4\Project1.dpr
  $(DCC)

Exemplo5.exe: Exemplo5.dpr
  $(DCC)


